<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.czaeshop.dao.ProductPictureDao">
    <select id="getPicturesByProductId" resultType="com.czaeshop.pojo.ProductPicture">
        select * from `t_product_pictures` where `productId` = #{productId}
    </select>
</mapper>